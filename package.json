{
  "name": "react--first-exercise",
  "version": "1.0.0",
  "description": "",
  "main": "index.html",
  "scripts": {
    "test": "npm-run-all test:*",
    "test:html": "globstar nu-html-checker *.html",
    "init-project": "npm install && npm-run-all init:*",
    "init:files": "touch README.md index.html",
    "init:gitignore": "curl https://raw.githubusercontent.com/github/gitignore/master/Node.gitignore -o .gitignore",
    "build": "npm-run-all build:* test",
    "build:autoprefixer": "autoprefixer-cli style.css",
    "watch": "npm-run-all build -p watch:*",
    "watch:sassprefixer": "onchange -d 100 sass/*.scss -- npm-run-all -p build:sass build:autoprefixer",
    "watch:browsersync": "browser-sync start --server --files components/*.js *.js *.html"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer-cli": "^1.0.0",
    "browser-sync": "^2.24.6",
    "globstar": "^1.0.0",
    "node-sass": "^4.9.2",
    "npm-run-all": "^4.1.3",
    "nu-html-checker": "^0.1.0",
    "onchange": "^4.1.0",
    "rimraf": "^2.6.2"
  },
  "dependencies": {}
}
